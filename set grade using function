
# function 
# function defination
def set_grade(p):
    if(p>90):
        print("A")
    elif(p>80 and p<90):
        print('B')
    elif(p>70 and p<80):
        print("C")
    elif(p>60 and p<70):
        print('D')
    else:
        print("E")
 #main function       
p=76.5
p2=90.0
p3=65.25
set_grade(p)
set_grade(p2)
set_grade(p3)
